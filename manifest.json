{
  "name": "Capacita Connection Manager",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "permissions": [
    "serial",
    {"socket": [
      "tcp-connect",
      "tcp-listen"]}
  ],
  "minimum_chrome_version": "23",
  "icons": {
    "16": "noun_25290_16_cc.png",
    "128": "noun_25290_128_cc.png"
  },
  "app": {
    "background": {
      "scripts": ["launch.js"],
      "transient": true
    }
  }
}
